{
  "name": "collectd3",
  "description": "d3js visualizations for collectd stats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "3.x",
    "async": "0.2.8",
    "lodash": "1.3.1",
    "mech-config": "git://github.com/mechanica/Config.git#master",
    "yaml": "0.2.3",
    "forever": "0.10.8",
    "rrd": "git://github.com/mechanica/node_rrd.git#master"
  },
  "scripts": {
    "test": "env NODE_ENV=test node_modules/.bin/mocha",
